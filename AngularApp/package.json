{
  "name": "despesas-frontend-angular",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "build": "ng build",
    "build:dev": "ng build --configuration=development",
    "watch": "ng build --watch --configuration development",
    "test": "set BROWSER=ChromeHeadless && ng test --browsers=ChromeHeadless --no-watch",
    "test:debug": "set BROWSER=Chrome && ng test --browsers=Chrome",
    "test:coverage": "ng test --code-coverage --browsers=ChromeHeadless --no-watch",
    "test:watch": "ng test --code-coverage --browsers=ChromeHeadless --watch",
    "prestart": "node aspnetcore-https",
    "start:windows": "ng serve --ssl --configuration local-development --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --configuration local-development --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.3",
    "@angular/cdk": "^18.0.3",
    "@angular/common": "^18.0.3",
    "@angular/compiler": "^18.0.3",
    "@angular/core": "^18.0.3",
    "@angular/forms": "^18.0.3",
    "@angular/material": "^18.0.3",
    "@angular/material-moment-adapter": "^18.0.3",
    "@angular/platform-browser": "^18.0.3",
    "@angular/platform-browser-dynamic": "^18.0.3",
    "@angular/router": "^18.0.3",
    "@date-io/dayjs": "^2.17.0",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@popperjs/core": "^2.11.8",
    "angular-datatables": "^16.0.1",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "chart.js": "^4.4.3",
    "chartjs-plugin-datalabels": "^2.2.0",
    "datatables.net": "^1.13.7",
    "datatables.net-dt": "^1.13.7",
    "datatables.net-select": "^1.7.0",
    "datatables.net-select-dt": "^1.7.0",
    "dayjs": "^1.11.10",
    "jquery": "^3.7.1",
    "mdb-ui-kit": "^7.3.2",
    "moment": "^2.30.1",
    "ng2-charts": "^5.0.4",
    "ng2-currency-mask": "^13.0.3",
    "ngx-mask": "^16.4.2",
    "oidc-client": "^1.11.5",
    "run-script-os": "*",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.4",
    "@angular/cli": "^18.0.3",
    "@angular/compiler-cli": "^18.0.3",
    "@angular/localize": "^18.0.3",
    "@types/bootstrap": "^5.2.6",
    "@types/jasmine": "~4.3.0",
    "@types/jquery": "^3.5.27",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.55.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-evaluator": "^1.2.0",
    "typescript": "~5.4.5"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
}
